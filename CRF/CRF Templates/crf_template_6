#First Word
#U03:%x[0,5]

#Date
U04:%x[0,4]


#Window of NER Tags
U05:%x[0,12]


#Window of Root Tags
U06:%x[0,2]
U07:%x[-1,2]
U07:%x[1,2]

#Window of Suffix Tags
#U08:%x[0,3]
#U09:%x[-1,3]
#U010:%x[1,3]

#POS Tag
U11:%x[-2,1]
U12:%x[-1,1]
U13:%x[0,1]
U14:%x[1,1]
U15:%x[2,1]


U20:%x[0,2]%x[0,1]

#suffix bigrams
U30:%x[0,2]%x[0,3]
U31:%x[0,1]%x[0,3]


